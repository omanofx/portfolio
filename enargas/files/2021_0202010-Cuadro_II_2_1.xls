    function pdfdownload (NomArchivo)
    {
        //alert("111");
        html2canvas(document.body, {
            onrendered: function (canvas) {
                var data = canvas.toDataURL();
                var docDefinition = {
                    content: [{
                        image: data,
                        width: 500,
                    }]
                };
                pdfMake.createPdf(docDefinition).download(NomArchivo);
            }
        });
        //cerrar();
    }
